{
  "scanSettings": {
    "baseBranches": ["master"],
    "resolveAllDependencies": true,
    "npm.resolveDependencies": true,
    "maven.resolveDependencies": true,
    "includes": [
      "**/package.json",
      "**/package-lock.json",
      "**/pom.xml"
    ]
  },
  "checkRunSettings": {
    "vulnerableCheckRunConclusionLevel": "failure",
    "displayMode": "diff",
    "useMendCheckNames": true
  },
  "issueSettings": {
    "minSeverityLevel": "LOW",
    "issueType": "DEPENDENCY"
  }
}